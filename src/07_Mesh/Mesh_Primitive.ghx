                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">C# MCol</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">true</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This applies a list of input colors to the vertices of a Mesh.

    // Let's first try to parse the inputs based on whether if they are Color objects
    // or strings representing named colors.
    List&lt;Color&gt; colors = new List&lt;Color&gt;();
    for (int i = 0; i &lt; C.Count; i++) {
      try {
        object c = C[i];
        // Is it a Color object?
        if (c is Color) {
          Color clr = (Color) c;
          colors.Add(clr);
        }
          // is it a string object?
        else if (c is string) {
          string name = (string) c;
          Color clr = Color.FromName(name);
          colors.Add(clr);
        }
      }
      catch {
        Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Data conversion to Color failed");
      }
    }

    // Reset previous colors in the Mesh.
    Msh.VertexColors.Clear();

    // Apply colors sequentially to the Mesh, using the modulo
    // operator to wrap around the list
    int colorCount = colors.Count;
    for (int i = 0; i &lt; Msh.Vertices.Count; i++) {
      int colorId = i % colorCount;
      Msh.VertexColors.Add(colors[colorId]);
    }

    // Outputs
    M = Msh;</item>
