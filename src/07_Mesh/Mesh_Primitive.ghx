                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This component creates a Mesh from scratch, from a set of input
    // vertices, faces and vertex colors.

    // C# Script components cannot take MeshFace as inputs,
    // so use the generic `object` type and cast it to MeshFace objects.
    List&lt;MeshFace&gt; faces = new List&lt;MeshFace&gt;();
    try {
      for (int i = 0; i &lt; F.Count; i++) {
        faces.Add((MeshFace) F[i]);
      }
    }
    catch {
      Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Data conversion to MeshFace failed");
    }

    // Start an empty Mesh.
    Mesh m = new Mesh();

    // Add the vertices.
    m.Vertices.AddVertices(V);

    // Add the faces, making sure the vertex indices are within range.
    int maxI = V.Count - 1;
    for (int i = 0; i &lt; faces.Count; i++) {
      MeshFace f = faces[i];
      if (f.A &gt; maxI || f.B &gt; maxI || f.C &gt; maxI || f.D &gt; maxI) {
        Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, "Face vertex exceeds mesh vertex count.");
      }
      else {
        m.Faces.AddFace(f);
      }
    }

    // Add vertex colors.
    for (int i = 0; i &lt; C.Count; i++) {
      m.VertexColors.Add(C[i]);
    }
    
    // Outputs.
    M = m;</item>
