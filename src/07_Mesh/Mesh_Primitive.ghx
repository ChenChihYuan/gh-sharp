                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This component creates a plane-like Mesh.
    // This example was written before finding out the RhinoCommon way:
    //   Mesh mesh = Mesh.CreateFromPlane(B.Plane, B.X, B.Y, W, H);

    // Let's start with an empty Mesh:
    Mesh mesh = new Mesh();

    // Let's get the Rectangle's main orientation Plane.
    Plane pl = B.Plane;

    // Let's calculate the quad dimensions on each direction.
    double dx = B.X.Length / W;
    double dy = B.Y.Length / H;

    // Let's create a plane on the lower-left corner of the rectangle,
    // and use it as the main coordinate system to generate a planar mesh.
    Plane pl0 = new Plane();
    pl0.Origin = pl.PointAt(-0.5 * B.X.Length, -0.5 * B.Y.Length, 0);
    pl0.XAxis = pl.XAxis;
    pl0.YAxis = pl.YAxis;
    AddGridToMesh(mesh, pl0, W, dx, H, dx);

    // Mesh area.
    double area = B.X.Length * B.Y.Length;

    M = mesh;
    A = area;</item>
