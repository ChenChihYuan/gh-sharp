                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This component creates a box-like Mesh.
    // This example was written before finding out the RhinoCommon way:
    //   Mesh mesh = Mesh.CreateFromBox(B, X, Y, Z);

    // Let's start with an empty Mesh:
    Mesh mesh = new Mesh();

    // Let's get the Box's main orientation Plane.
    Plane pl = B.Plane;

    // Let's calculate the quad dimensions on each direction.
    double dx = B.X.Length / X;
    double dy = B.Y.Length / Y;
    double dz = B.Z.Length / Z;

    // Bottom side
    Plane pl0 = new Plane();
    pl0.Origin = pl.PointAt(-0.5 * B.X.Length, -0.5 * B.Y.Length, -0.5 * B.Z.Length);
    pl0.XAxis = pl.YAxis;
    pl0.YAxis = pl.XAxis;
    AddGridToMesh(mesh, pl0, Y, dy, X, dx);

    // Front side
    Plane pl1 = new Plane();
    pl1.Origin = pl.PointAt(-0.5 * B.X.Length, -0.5 * B.Y.Length, -0.5 * B.Z.Length);
    pl1.XAxis = pl.XAxis;
    pl1.YAxis = pl.ZAxis;
    AddGridToMesh(mesh, pl1, X, dx, Z, dz);

    // Right side
    Plane pl2 = new Plane();
    pl2.Origin = pl.PointAt(0.5 * B.X.Length, -0.5 * B.Y.Length, -0.5 * B.Z.Length);
    pl2.XAxis = pl.YAxis;
    pl2.YAxis = pl.ZAxis;
    AddGridToMesh(mesh, pl2, Y, dy, Z, dz);

    // Back side
    Plane pl3 = new Plane();
    pl3.Origin = pl.PointAt(0.5 * B.X.Length, 0.5 * B.Y.Length, -0.5 * B.Z.Length);
    pl3.XAxis = -pl.XAxis;
    pl3.YAxis = pl.ZAxis;
    AddGridToMesh(mesh, pl3, X, dx, Z, dz);

    // Left side
    Plane pl4 = new Plane();
    pl4.Origin = pl.PointAt(-0.5 * B.X.Length, 0.5 * B.Y.Length, -0.5 * B.Z.Length);
    pl4.XAxis = -pl.YAxis;
    pl4.YAxis = pl.ZAxis;
    AddGridToMesh(mesh, pl4, Y, dy, Z, dz);

    // Top side
    Plane pl5 = new Plane();
    pl5.Origin = pl.PointAt(-0.5 * B.X.Length, -0.5 * B.Y.Length, 0.5 * B.Z.Length);
    pl5.XAxis = pl.XAxis;
    pl5.YAxis = pl.YAxis;
    AddGridToMesh(mesh, pl5, X, dx, Y, dy);

    F = mesh;</item>
