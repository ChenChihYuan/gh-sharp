                    <item name="IconDisplay" type_name="gh_int32" type_code="3">1</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">7dd31f46-9ccc-41c7-8c27-99a86e5652a3</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">C# MSphere</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">true</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This component creates a sphere-like Mesh.
    // This example was written before finding out the RhinoCommon way:
    //   Mesh mesh = Mesh.CreateFromSphere(...);

    // In the original GH component, U and V are inverted: U is latitude (pole to pole),
    // and V is longitude (around the sphere). The values here are also inverted to
    // maintain consistency.

    // Some checks first:
    if (U &lt; 3) {
      Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Insufficient vertices in the u-direction.");
      return;
    }
    if (V &lt; 2) {
      Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Insufficient vertices in the v-direction.");
      return;
    }

    // The original component overrides V as 2, and turns it to 3.
    if (V == 2) {
      V = 3;
    }

    // Let's start with an empty Mesh:
    Mesh mesh = new Mesh();

    // Calculate the angle increments in the U (latitude) and V (longitude) directions in radians
    double du = Math.PI / U;
    double dv = 2.0 * Math.PI / V;

    // Let's create all the vertices!
    // South Pole
    Point3d sp = B.PointAt(0, 0, -R);
    mesh.Vertices.Add(sp);

    double x, y, z;
    Point3d p;
    // Iterate over the parallels
    for (int j = 1; j &lt; U; j++) {
      z = R * Math.Sin(-0.5 * Math.PI + j * du);

      // Iterate over the meridians
      for (int i = 0; i &lt; V; i++) {
        x = R * Math.Cos(-0.5 * Math.PI + j * du) * Math.Cos(i * dv);
        y = R * Math.Cos(-0.5 * Math.PI + j * du) * Math.Sin(i * dv);

        p = B.PointAt(x, y, z);

        mesh.Vertices.Add(p);
      }
    }

    // North Pole
    Point3d np = B.PointAt(0, 0, R);
    mesh.Vertices.Add(np);

    // Now, let's create the faces by matching vertex indices.
    // Let's start with the south crown (triangles).
    int a, b, c, d;
    for (int i = 0; i &lt; V; i++) {
      // Remember to maintain CCW orientation of the face.
      a = 0;
      c = 1 + i;

      // Indices are different if the triangle is last on the row.
      if (i == V - 1) {
        b = 1;
      }
      else {
        b = i + 2;
      }

      mesh.Faces.AddFace(a, b, c);
    }

    // Let's do the parallel rows now (quads).
    for (int j = 0 ; j &lt; U - 2; j++) {
      for (int i = 0; i &lt; V; i++) {
        // First corner
        a = 1 + j * V + i;

        // Indices are different if the quad is last on the row.
        if (i == V - 1) {
          b = a - V + 1;
          c = a + 1;
          d = a + V;
        }
        else {
          b = a + 1;
          c = a + V + 1;
          d = a + V;
        }

        mesh.Faces.AddFace(a, b, c, d);
      }
    }

    // And finally, let's top the north crown (triangles).
    for (int i = 0; i &lt; V; i++) {
      // Remember to maintain CCW orientation of the face.
      a = 1 + V * (U - 2) + i;
      c = 1 + V * (U - 1);

      // Indices are different if the triangle is last on the row.
      if (i == V - 1) {
        b = a - V + 1;
      }
      else {
        b = a + 1;
      }

      mesh.Faces.AddFace(a, b, c);
    }

    // Outputs
    M = mesh;</item>
