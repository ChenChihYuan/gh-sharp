                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">1d66775a-2c4b-47e4-8772-7ac4a1f406ee</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">C# MSphereEx</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">true</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @garciadelcastillo

    // This component creates a quad mesh sphere.

    // The RhinoCommon implementation differs from the original, as the
    // RhinoCommon method uses quad subdivisions, rather than face count:
    //   Sphere s = new Sphere(B, R);
    //   Mesh mesh = Mesh.CreateQuadSphere(s, C);

    // Instead, for this component, we will create the quad mesh sphere manually
    // by starting from a cube mesh, and "pushing" the vertices normal to the
    // center of the sphere, so that their distance to the center equals the radius.

    // Let's create a box mesh of side 1 unit, and faces equal to the input.
    Interval interv = new Interval(-0.5, 0.5);
    Box box = new Box(B, interv, interv, interv);
    Mesh mesh = Mesh.CreateFromBox(box, C, C, C);
    
    // "Push" each vertex so that it's distance to the center equals the radius of the sphere.
    // Since mesh vertices have float coordinates (Point3f), we will use float coordinates for 
    // calculations. 
    Point3f center = (Point3f) B.Origin;
    for (int i = 0; i &lt; mesh.Vertices.Count; i++) {
      Point3f pt = mesh.Vertices[i];
      Vector3f v = pt - center;
      float factor = (float) (R / v.Length);
      Point3f ptR = center + factor *v;
      mesh.Vertices[i] = ptR;
    }

    // Outputs
    M = mesh;</item>
