  // This function takes a Mesh, and adds a planar grid of vertices+faces to it.
  // Takes the plane containing the grid, face count in each direction, and face dimensions.
  void AddGridToMesh(Mesh m, Plane pl, int facesX, double dx, int facesY, double dy) {
    // Read how many vertices the Mesh had before more are added.
    int vc = m.Vertices.Count;

    // Add new vertices.
    Point3d v;
    for (int j = 0; j &lt;= facesY; j++) {
      for (int i = 0; i &lt;= facesX; i++) {
        v = pl.PointAt(i * dx, j * dy);
        m.Vertices.Add(v);
      }
    }

    // Add new faces by matching vertices counter-clockwise based on their row/column indices.
    int a, b, c, d;
    for (int j = 0; j &lt; facesY; j++) {
      for (int i = 0; i &lt; facesX; i++) {
        a = vc + j * (facesX + 1) + i;
        b = a + 1;
        c = b + facesX + 1;
        d = c - 1;

        m.Faces.AddFace(a, b, c, d);
      }
    }
  }</item>
