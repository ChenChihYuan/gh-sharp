                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // Flatten and combine a collection (Lists or Trees) of data.

    // We call a ScheduleSolution used to modify the naming of the inputs.
    // Schedule a new solution after a specified time interval.
    // This means, that after the component has run, a new solution will be called to solve the specific operations we are passing through the CallBack method.
    // https://discourse.mcneel.com/t/can-t-quite-figure-out-how-to-use-schedulesolution/105590/3?u=bianchini
    GrasshopperDocument.ScheduleSolution(5, CallBack);

    // Global parameter.
    GH_Structure&lt;IGH_Goo&gt; tree = new GH_Structure&lt;IGH_Goo&gt;();
    int numberOfInputs = this.Component.Params.Input.Count;
    // We loop through the inputs.
    for (int i = 0; i &lt; numberOfInputs; i++)
    {
      // We collect the input as a GH_Structure.
      GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[i].VolatileData;
      // We merge the inputTree one by one in our global variable, using the method MergeStructure.
      // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_Data_GH_Structure_1_MergeStructure.htm
      tree.MergeStructure(inputTree);
    }

    // Output result.
    // Using our private method we convert the GH_Structure in a DataTree.
    R = StructureToDataTree(tree);
