                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // Private property.
  private bool _flattenInputs;

  // Method used to modify the input names.
  public void VariableParameterMaintenance()
  {
    // We loop through the inputs.
    int num = this.Component.Params.Input.Count - 1;
    for (int index = 1; index &lt;= num; ++index)
    {
      // If it is true we rename the inputs using the prefix F for flatten.
      if(_flattenInputs)
      {
        this.Component.Params.Input[index].Name = string.Format("Branch {0}", (object) index);
        this.Component.Params.Input[index].NickName = string.Format("F{0}", (object) index);
      }
        // Otherwise we rename the inputs with the prefix B for Graft.
      else
      {
        this.Component.Params.Input[index].Name = string.Format("Branch {0}", (object) index);
        this.Component.Params.Input[index].NickName = string.Format("G{0}", (object) index);
      }
      // We set the input accessing the tree, this is the same action we do when we right click on a input and we select if
      // it has to be a item a list of a tree.
      this.Component.Params.Input[index].Access = GH_ParamAccess.tree;
    }
  }
  // Inside the CallBack we call the method we defined to change the input names, and we expire the component solution so
  // in the next new solution this component can be solved again.
  // It is necessary define the CallBack method with a GH_Document input otherweise the ScheduleSolution won't work.
  public void CallBack(GH_Document gh)
  {
    VariableParameterMaintenance();
    this.Component.Params.OnParametersChanged();
    this.Component.ExpireSolution(false);
  }</item>
