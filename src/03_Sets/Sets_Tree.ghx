                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component shifts the indices for all the paths.

    // Glabal variables.
    // Collect the input structure.
    // Instantiate a DataTree.
    // Collect the dimension of the structure, number of branches.
    // Collect the input shift and ensure is positive.
    GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[0].VolatileData;
    DataTree&lt;IGH_Goo&gt; tree = new DataTree&lt;IGH_Goo&gt;();
    int treeCapacity = inputTree.Branches.Count;
    int shift = Math.Abs(O);

