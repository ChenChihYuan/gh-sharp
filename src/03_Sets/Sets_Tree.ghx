                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component performs lexical operations on data trees.
    // For more information, the help description is very exhaustive and clear in the vanilla component.
    // As per other components we are not able to recreate the same interaction, but we were been able to reproduce the same functionalities.

    // Glabal variables.
    // We collect the input tree as an IGH_Goo structure so we can manage all the types of data, null, empty, etc.
    // We initialize the output tree.
    // We collect the path length of the first branch of the tree.
    // We initialize a GH_LexerCode
    // https://developer.rhino3d.com/api/grasshopper/html/T_Grasshopper_Kernel_Data_GH_LexerCombo.htm
    GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[0].VolatileData;
    GH_Structure&lt;IGH_Goo&gt; outputTree = new GH_Structure&lt;IGH_Goo&gt;();
    int pathLength = T.Paths[0].Length;
    GH_LexerCombo lexers = new GH_LexerCombo();

    // We generate the tree path as a lexer code, using a function we defined.
    string pathLexerCode = GetPathLexerCode(pathLength);

    // We use a switch statement to recreate the options pre-defined by clicking the right button of the mouse on the vanilla component.
    // https://docs.microsoft.com/en-us/dotnet/csharp/language-reference/keywords/switch
    // Setting the mapping options on editor mode, you can perform lexical operation using the two attached panels.
    switch(MappingOptions)
    {
      // Editor mode, we are using the inputs in the panels.
      // We move all the login in a loop and we exit when is finished.
      case 0:
        for (int i = 0; i &lt; Source.Count; i++)
        {
          lexers = new GH_LexerCombo(Source[i], Target[i]);
          GH_Lexer.PerformLexicalReplace(lexers.Source, lexers.Target, inputTree, outputTree);
          O = StructureToDataTree(outputTree);
        }
        return;
      // Null solution, we are using the same lexical pattern for the source and the target.
      case 1:
        lexers = new GH_LexerCombo(pathLexerCode, pathLexerCode);
        break;
      // Flatten solution the target is a flat list descibed as {0}.
      case 2:
        lexers = new GH_LexerCombo(pathLexerCode, "{0}");
        break;
      // Graft solution, every data is moved in a branch.
      // So the source has to be {X;X}(i) and the target {X;X;i}.
      case 3:
