                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component graft the data in a new data tree.

    // Global parameters
    DataTree&lt;object&gt; tree = new DataTree&lt;object&gt;();
    List&lt;object&gt; branch = null;

    // If the branch is not composed of a single branch, meaning a list, we enter in this statement.
    if(T.BranchCount &gt; 1)
    {
      // We loop through each branch.
      for (int j = 0; j &lt; T.BranchCount; j++)
      {
        // We extract each branch in a list.
        branch = T.Branch(j);
        // If the branch is null or doesn't have any data, we jump in this statement.
        if(branch == null || branch.Count == 0)
        {
          // If input E is true, so we want to consider null values, we enter in this statement.
          if(E)
          {
            // First thing we do is to create a path.
            // We take the path of the branch and we add an index path of value zero.
            // We add to our tree an empty list and the path defined.
            GH_Path path = T.Path(j).AppendElement(0);
            tree.AddRange(new List&lt;object&gt;(), path);
          }
        }
        else
        {
          // If the branch is not null, so we loop through all the data in the branch.
          for (int k = 0; k &lt;= branch.Count - 1; k++)
          {
            // We create a new path for every single item, appending to the branch path the index of the loop.
            GH_Path path = T.Path(j).AppendElement(k);
            // If the value at index k in the branch is not null OR N is true so we want to consider null values.
            // We add to the tree the value with the new path.
            if(branch[k] != null || N)
              tree.Add(branch[k], path);
          }
        }
      }
    }
      // if the branch is a single list.
    else
    {
      // We take the branch.
      branch = T.Branch(0);
      // We loop for all the elements has inside.
      for (int j = 0; j &lt; branch.Count; j++)
      {
        // If N input is false and the value at the index j is null we skip it and jump to the next interaction of the loop.
        if(!N &amp;&amp; branch[j] == null)
          continue;
        // We create a new path for every single element of the list, appending the index j to the path.
        GH_Path path = T.Path(0).AppendElement(j);
        // We add each element in a separate branch defined by the path.
        tree.Add(branch[j], path);
      }
    }
    // Output result.
    Tr = tree;</item>
