                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component extracts all the branches from a tree.

    // Using a C#script is very inconvenient to try to define a dynamic behavior of the component,
    // but we tried to mimic as much as possible the original component.
    // We extract the match outputs in a boolean input.
    // What the component is able to do:
    // 1. Pressing the button, the component updates the output to the number of branches of the tree.
    // 2. Connecting a different input the component is able to recognize if the number of branches is less of the outputs and
    // identify this difference updating the name of the output as Empty Branch (EB).
    // What the component is not able to do:
    // 1. Reducing the number of outputs base on the number of branches requires a manual remotion of the output identifies with the name EB.

    // NOTE
    // Since we are working with dynamic outputs in this component,
    // the variable to hold the outputs, _tree is defined
    // as a persistent variable, outside the scope of the RunScript()
    // function. Also, from the _tree, the outputs from each
    // branch are passed to their respective output parameters in
    // the AfterRunScript()function. Hence, before every execution
    // of the RunScript() function, we manually clear out all
    // the branches and data in the outputTree.
    // We identify the internal variable using underscore.
    _tree.Clear();

    // Collect number of branches and number of outputs.
    int numberOfBranchInput = D.BranchCount;
    int numberOfOutputs = this.Component.Params.Output.Count;
    // Check is the number of branches is bigger than the number of outputs, if it is true, throw a warning message.
    if(numberOfBranchInput &gt; numberOfOutputs)
    {
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, "Tree contains more branches than this component has output parameters.");
    }

    // Passing a true value, using the button, we trigger the creation of the outputs based on the number of branches.
    if(MatchOutputs)
    {
      for (int i = numberOfOutputs; i &lt; numberOfBranchInput; i++)
      {
        // Instantiation of a generic object parameter.
        // https://developer.rhino3d.com/api/grasshopper/html/fbfe5e40-ba8d-4e53-97c6-27572e049835.htm
        // To do this we have to add to the using list Grasshopper.Kernel.Parameters;
        Param_GenericObject param = new Param_GenericObject();
        // The nickname of an input or output is the visible 'name' on the component.
        param.NickName = string.Format("B{0}", i);
        param.Name = string.Format("B{0}", i);
        // We specify the type of access the output can support, all the data will be treated at the same time as a list.
        param.Access = GH_ParamAccess.list;
        // We add the parameter to the outputs, using the method RegisterOutputParam, which ask for a IGH_Param, so we execute a cast.
        // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_GH_ComponentParamServer_RegisterOutputParam.htm
        this.Component.Params.RegisterOutputParam((IGH_Param) param);
      }
    }

    // We loop for the number of outputs the component has.
    for (int i = 0; i &lt;= numberOfOutputs; i++)
    {
      // If index i is bigger than the number of branches, we replace the nickName of the output with EB (Empty Branch).
      if(i &gt;= numberOfBranchInput)
        this.Component.Params.Output[i].NickName = string.Format("EB{0}", i);
      else
        // We populate the persistante variable.
        _tree.AddRange(D.Branch(i), D.Path(i));
    }</item>
