                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // This function greats a lexer code based on the dimension of the tree path.
  // As you can see, this function is repeated twice, but with a different signature,
  // this is what is called function overloading:
  // it is a process of creating multiple methods in a class with the same name but with a different signature.
  private string GetPathLexerCode(int pathLength)
  {
    // We initialize an array as bigger as the input passed.
    char[] pathCaracthers = new char[pathLength];
    // We loop for the quantity of the input, and we collect in the array
    // the character derived from the integer value.
    // The Latin capital letter A in the Unicode table is the decimal number 65.
    for (int i = 0; i &lt; pathLength; i++)
      pathCaracthers[i] = (char) (i + 65);
    // We return the Lexur code as a string in the specific forma required, example {A;B}.
    return string.Format("{{{0}}}", string.Join(";", pathCaracthers));
  }

  // We are doing the same process of the function above, with the only difference that we have
  // a trimmer input used to reduce the result by the value used.
  private string GetPathLexerCode(int pathLength, int trimmer)
  {
    char[] pathCaracthers = new char[pathLength - trimmer];

    for (int i = 0; i &lt; pathLength - trimmer; i++)
      pathCaracthers[i] = (char) (i + 65);

    return string.Format("{{{0}}}", string.Join(";", pathCaracthers));
  }

  // Private method to convert a GH_Structure in DataTree.
  private DataTree&lt;IGH_Goo&gt; StructureToDataTree(GH_Structure&lt;IGH_Goo&gt; structure)
  {
    // We instantiate a tree.
    DataTree&lt;IGH_Goo&gt; tree = new DataTree&lt;IGH_Goo&gt;();
    // We loop through each branch and copied in the DataTree.
    for(int i = 0;i &lt; structure.Branches.Count;++i)
    {
      tree.AddRange((List&lt;IGH_Goo&gt;) structure.get_Branch(i), structure.get_Path(i));
    }
    // We return the tree.
    return tree;
  }</item>
