                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // Persistante variables.
  // A boolean value, to trigger some behaviors we have inside the AfterRunScript.
  // A integer value used in the behaviors.
  private bool _flag;
  private int _gateValue;
  // This method will be called once every solution, after any calls to RunScript.
  // https://discourse.mcneel.com/t/output-addvolatiledata-in-c-script-component-overridden-with-null-element/113520/2?u=bianchini
  public override void AfterRunScript()
  {
    // We loop through the input to update the nickName of each of them.
    // We avoid changing the first input (index 0) and we started from the second (index 1).
    for (int i = 1; i &lt; this.Component.Params.Input.Count; i++)
      this.Component.Params.Input[i].NickName = "A" + i;

    // If the flag is true trigger the AfterRunScript and we jamp in this condition.
    if(_flag)
    {
      // We clear the data to avoid null values where passed by the RunScript in the output.
      this.Component.Params.Output[0].ClearData();
      // We pass the data from the input to the output.
      this.Component.Params.Output[0].AddVolatileDataTree(this.Component.Params.Input[_gateValue + 1].VolatileData);
      // We rename the output adding the index of the input we are using.
      this.Component.Params.Output[0].NickName = string.Format("S{0}", _gateValue);
    }
    else
    {
      // If the flag is negative, we rename the output with a generic nickName and we expire the output.
      this.Component.Params.Output[0].NickName = "S";
      this.Component.Params.Output[0].ExpireSolution(true);
    }
  }
