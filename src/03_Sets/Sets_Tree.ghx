    for (int i = 0; i &lt; treeCapacity; i++)
    {
      // Collect the path capacity, the length of the path.
      int pathCapacity = inputTree.get_Path(i).Length;
      // If the input shift is equal or bigger than the path capacity
      // we throw an error message, and we step to the next iteration.
      if(shift &gt;= pathCapacity)
      {
        this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, string.Format("Path {0} was shifted out of existence", inputTree.get_Path(i)));
        continue;
      }
      else
      {
        // Get the path of the branch.
        GH_Path path = inputTree.get_Path(i);
        // Loop for the value of the shift
        for (int j = 0; j &lt; shift; j++)
        {
