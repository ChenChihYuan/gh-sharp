                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // Flatten and combine a collection (Lists or Trees) of data.
    // We replace the right click event with a bool input.
    // We can mimic the same input namings because C#script will spit out a serious of errors.
    // We will use Fn to specify we are flattening the structure and Gn when we keep the structure.
    // To mimic as much as possible the component we added also the message below the component.

    // First think we do is to pass the bolean input to the private input we have in the area custom additional code.
    // Second we call a ScheduleSolution used to modify the naming of the inputs.
    // https://discourse.mcneel.com/t/can-t-quite-figure-out-how-to-use-schedulesolution/105590/3?u=bianchini
    _flattenInputs = FlattenInputs;
    GrasshopperDocument.ScheduleSolution(5, CallBack);

    // Glabal variables.
    DataTree&lt;IGH_Goo&gt; tree = new DataTree&lt;IGH_Goo&gt;();
    // We loop throught the number of inputs.
    int numberOfInputs = this.Component.Params.Input.Count;
    for (int i = 1; i &lt; numberOfInputs; i++)
    {
      // We collect the data, extracting them directly from the inputs and we cast them in GH_Structure&lt;IGH_Goo&gt;
      GH_Structure&lt;IGH_Goo&gt; inputStru = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[i].VolatileData;
      // If it is true
      if(FlattenInputs)
      {
        // We modify the message below the component.
        this.Component.Message = "Flatten";
        // We flatten the data in a unique list, using the FlattenData method.
        inputStru.FlattenData();
        // We set the new index path for this data structure.
        // Here we can see another way to structure the path, using an integer array.
        // The definition of the path is the order of the values in the array.
        int[] indexPath = new int[2]{0,i - 1};
        GH_Path path = new GH_Path(indexPath);
        // We add the branch to the tree.
        tree.AddRange(inputStru, path);
      }
        // it it is false.
      else
      {
        // We modify the message below the component
        this.Component.Message = "Graft";
        // We loop throught the number of paths or branches.
        int pathLength = inputStru.PathCount;
        for (int j = 0; j &lt; pathLength; j++)
        {
          // We collect the branch in a list.
          List&lt;IGH_Goo&gt; branch = inputStru.Branches[j];
          // Here we are prepending a index to the path of the branch.
          GH_Path path = inputStru.Paths[j].PrependElement(i - 1);
          // We add the branch to the tree.
          tree.AddRange(branch, path);
        }
      }
    }

    // Output result.
    R = tree;</item>
