                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // Persistante variable.
  DataTree&lt;object&gt; _tree = new DataTree&lt;object&gt;();
  // This method will be called once every solution, after any calls to RunScript.
  // https://discourse.mcneel.com/t/output-addvolatiledata-in-c-script-component-overridden-with-null-element/113520/2?u=bianchini
  public override void AfterRunScript()
  {
    // Loop through the ouputs
    int numberOfOutputs = this.Component.Params.Output.Count;
    for (int i = 0; i &lt; numberOfOutputs; i++)
    {
      // If index i is bigger than branch count return.
      if(i &gt;= _tree.BranchCount) return;
      // Because we are in an after process,
      // we have to clean the outputs that have been set with null value during the primary process
      // and after we add our data.
      this.Component.Params.Output[i].ClearData();
      this.Component.Params.Output[i].AddVolatileDataList(_tree.Path(i), _tree.Branch(i));
    }
  }
</item>
