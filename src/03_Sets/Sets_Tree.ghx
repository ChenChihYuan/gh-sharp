                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component provide some information regarding the tree, paths, branch lengths, number of branches.

    // Global parameters.
    GH_Path[] paths = new GH_Path[T.BranchCount];
    int[] branchLength = new int[T.BranchCount];
    // Looping through the branches.
    for (int i = 0; i &lt; T.BranchCount; i++)
    {
      // Collecting branch path and count in the two arrays.
      paths[i] = T.Path(i);
      branchLength[i] = T.Branch(i).Count;
    }
    // Output resut.
    P = paths;
    L = branchLength;
    C = T.BranchCount;</item>
