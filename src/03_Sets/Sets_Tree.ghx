                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // This component removes all branches from a Tree that are smaller or bigger than the input thresholds provided.
    // For this component we will use the method RemovePath.
    // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_DataTree_1_RemovePath.htm

    // Looping through the DataTree.
    for (int i = 0; i &lt; T.BranchCount; i++)
    {
      // Record the path of the branch.
      GH_Path path = T.Path(i);
      // If the dimension of the branch is smaller than input N_0, remove the path.
      if(T.Branch(i).Count &lt; N_0)
        T.RemovePath(path);
        // If the dimension of the branch is bigger than input N_1, remove the path.
      else if(T.Branch(i).Count &gt; N_1)
        T.RemovePath(path);
    }
    // Output result.
    Tr = T;</item>
