                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component filters the input streams.
    // This component acts like a switch, only one of the input streams is allowed to pass through the component.

    // NOTE
    // Since we are working with dynamic outputs in this component,
    // the output parameter is set in the AfterRunScript()function. Hence, before every execution
    // of the RunScript() function, we manually clear out all the data in the output.
    // We identify the internal variable using underscore.

    // If the input G, input index 0, has more than one value.
    // We set the flag false, and throw an error.
    if(this.Component.Params.Input[0].VolatileData.DataCount &gt; 1)
    {
      _flag = false;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Filter component can only operate on a single Filter Index value.");
    }
      // If the input G value is less than zero.
      // We set the flag false, and throw an error.
    else if(G &lt; 0)
    {
      _flag = false;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Filter index too low.");
    }
      // If the input G value is bigger than the number of input.
      // We reduce the count by two because the list start with index zero and we don't want to consider the first input.
      // We set the flag false, and throw an error.
    else if(G &gt; this.Component.Params.Input.Count - 2)
    {
      _flag = false;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Filter index too high.");
    }
      // If we passed all the check we set the flag true and the gateValue equal to the G input.
    else
    {
      _flag = true;
      _gateValue = G;
    }</item>
