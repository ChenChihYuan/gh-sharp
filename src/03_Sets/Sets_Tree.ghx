                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // Persistante variables.
  // Two boolean values, to trigger some behaviors we have inside the AfterRunScript.
  // A integer value used in the behaviors.
  private bool _flag;
  private bool _flagGateInputs;
  private int _gateValue;

  // Private method to expire all the outputs we are not using.
  // We declare the integer value nullable adding a question mark after the type.
  // https://docs.microsoft.com/en-us/dotnet/csharp/language-reference/builtin-types/nullable-value-types
  // So passing null, the if statement will be ignored, and we expire all the outputs.
  // Other solution could be passing a negative value like -1.
  private void ExpireOutput(int? skipOutput)
  {
    for (int i = 0; i &lt; this.Component.Params.Input.Count; i++)
    {
      if(i == skipOutput)
        continue;
      this.Component.Params.Output[i].ExpireSolution(true);
    }
  }

  // This method will be called once every solution, after any calls to RunScript.
  // https://discourse.mcneel.com/t/output-addvolatiledata-in-c-script-component-overridden-with-null-element/113520/2?u=bianchini
  public override void AfterRunScript()
  {
    // We loop through the input to update the nickName of each of them.
    // We avoid changing the first input (index 0) and we started from the second (index 1).
    for (int i = 0; i &lt; this.Component.Params.Output.Count; i++)
      this.Component.Params.Output[i].NickName = "G" + i;

    if(_flagGateInputs)
    {
      // We clear the data to avoid null values where passed by the RunScript in the output.
      this.Component.Params.Output[0].ClearData();
      // We pass the data from the input to the output.
      this.Component.Params.Output[0].AddVolatileDataTree(this.Component.Params.Input[0].VolatileData);
      // We call our private method and we expire all the outputs except that the one with index 0
      ExpireOutput(0);
    }

    if(_flag)
    {
      // We clear the data to avoid null values where passed by the RunScript in the output.
      this.Component.Params.Output[_gateValue].ClearData();
      // We pass the data from the input to the output.
      this.Component.Params.Output[_gateValue].AddVolatileDataTree(this.Component.Params.Input[0].VolatileData);
      // We call our private method and we expire all the outputs except that the one passed as input G.
      ExpireOutput(_gateValue);
    }
    else
      // We call our private method and we expire all the outputs, passing null as the input of the argument.
      ExpireOutput(null);
  }
