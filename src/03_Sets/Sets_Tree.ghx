              <chunks count="1">
                <chunk name="Container">
                  <items count="9">
                    <item name="CustomUsing" type_name="gh_bool" type_code="1">true</item>
                    <item name="Description" type_name="gh_string" type_code="10">A C#.NET scriptable component</item>
                    <item name="IconDisplay" type_name="gh_int32" type_code="3">1</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">8af4e208-ca5a-4c27-a002-748a9183232d</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">GH# Unflatten</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">false</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component moves items back into branches.

    // Glabal variables.
    DataTree&lt;object&gt; tree = new DataTree&lt;object&gt;();
    int startingRange = 0;
    // We flat the input tree T, and we extract the first branch in a list.
    T.Flatten();
    List&lt;object&gt; flattenTree = T.Branch(0);

    // We looping through the branches of tree input G.
    for (int i = 0; i &lt; G.BranchCount; i++)
    {
      // Collecting branch path and count in two variables.
      GH_Path path = G.Path(i);
      int branchCount = G.Branch(i).Count;
      // If the length of the list, minus the startingRange incremented  every interaction, is less than the length of the branch
      // we throw an error message and the result produced until this point.
      if (flattenTree.Count - startingRange &lt; branchCount)
      {
        this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Flattened tree has fewer items than guide tree.");
        Tr = tree;
        return;
      }
      // We extract a subList using the method GetRange.
      // https://docs.microsoft.com/en-us/dotnet/api/system.collections.generic.list-1.getrange?view=net-5.0
      List&lt;object&gt; subSet = flattenTree.GetRange(startingRange, branchCount);
      // We add the subList to the new tree with the path provided by input G.
      tree.AddRange(subSet, path);
      // We increase the startingRange of the branch's length, so next time we will use GetRange, it will start
      // after the value-added in the previous interaction.
      // Using this way we don't remove items from the list, so we keep the input unmutated.
      startingRange += branchCount;
    }
    // If the length of the list minus the startingRange incremented every interaction is bigger than zero
    // we throw an error message.
    if(flattenTree.Count - startingRange &gt; 0)
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Flattened tree has more items than guide tree.");

    // Output result.
    Tr = tree;</item>
