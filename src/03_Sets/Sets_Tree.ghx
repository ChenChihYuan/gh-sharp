                <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
              </items>
              <chunks count="1">
                <chunk name="Container">
                  <items count="10">
                    <item name="CustomUsing" type_name="gh_bool" type_code="1">true</item>
                    <item name="Description" type_name="gh_string" type_code="10">A C#.NET scriptable component</item>
                    <item name="EditorPosition" type_name="gh_drawing_point" type_code="30">
                      <X>5</X>
                      <Y>148</Y>
                    </item>
                    <item name="IconDisplay" type_name="gh_int32" type_code="3">1</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">848bd801-7929-4b71-bf41-7b5112975f5b</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">GH# Flip</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">false</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component flips the data tree.
    // Thinking about the data tree like an excel file and you are swapping rows and columns.

    // Global variables.
    // Collecting the data input as a GH_Structure, as in previous example we have to cust the data.
    GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[0].VolatileData;
    // We collect the length of the first branch,
    // the number of branches,
    // the path length of the first branch and
    // we set a parameter check.
    int branchCapacity = inputTree.Branches[0].Count;
    int treeCapacity = inputTree.Branches.Count;
    int pathCapacity = inputTree.get_Path(0).Length;
    int indexPathChecks = -1;

    // If the path count is equal one we don't have to execute any checks.
    // We set out check parameter and we move to the foor loop.
    if(inputTree.PathCount == 1)
    {
      indexPathChecks = pathCapacity - 1;
    }
    else
    {
      // We loop using the tree dimension, the number of branches.
      for (int t = 1; t &lt; treeCapacity; t++)
      {
        // We execute a first check.
        // If the path length of each branch is different of the path length of the first branch
        // we throw an error message, we expire the solution so we don't output anything and we terminate the process.
        if(inputTree.get_Path(t).Length != pathCapacity)
        {
          this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "All paths in the input tree must have the same length");
          this.Component.Params.Output[0].ExpireSolution(true);
          return;
        }
        // We loop using the dimension of the path.
        for (int v = 0; v &lt; pathCapacity; v++)
        {
          // We compare the first branch index path at the index position v to the following branch index paths at the same index position v.
          // If are not the same, we jump in the statement.
          if(inputTree.get_Path(0)[v] != inputTree.get_Path(t)[v])
          {
            // We check if the path check value is less than zero or it's the same as index value v.
            if (indexPathChecks &lt; 0 || indexPathChecks == v)
            {
              // We set path check equal value v.
              // This is a simple strategy to verify if the values are the same, if aren't we jump in the else statement.
              indexPathChecks = v;
            }
            else
            {
              // As for the previous error, we throw an error message, expire the solution, and terminate the process.
              this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "All paths may only differ at a single locus");
              this.Component.Params.Output[0].ExpireSolution(true);
              return;
            }
          }
        }
      }
    }

    // Glabal variable.
    DataTree&lt;object&gt; tree = new DataTree&lt;object&gt;();

    // This part is the real logic of the component.
    // We have nested loops because we have to swap raws(branches) with columns(elements in the branches).
    // So we start the first loop using the number of elements.
    for (int i = 0; i &lt; branchCapacity; i++)
    {
      // We store the path of the first branch.
      // And we instantiate a new list every time we re-start the loop.
      GH_Path path = new GH_Path(inputTree.get_Path(0));
      List&lt;object&gt; tempBranch = new List&lt;object&gt;();
      // We enter in the second loop, using the branches.
      for (int j = 0; j &lt; treeCapacity; j++)
      {
        // We check if the branch is empty, or the index i (counting the branch capacity)
        // is bigger or equal to the branch count.
        // If it is true, we add to the list a null value.
        // Otherwise we add to the list the value we have from the branch.
        if(inputTree.Branches[j].Count == 0 || i &gt;= inputTree.Branches[j].Count)
          tempBranch.Add(null);
        else
          tempBranch.Add(inputTree.Branches[j][i]);
      }
      // The branch path is like an array of integer, so we can modify any values
      // accessing it by an index value.
      path[indexPathChecks] = i;
      // We add the list to the tree, with the modified path.
      tree.AddRange(tempBranch, path);
    }

    // Output result.
    Dr = tree;</item>
