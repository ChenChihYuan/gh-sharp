          // picking one of the two methods provided.
          // https://docs.microsoft.com/en-us/dotnet/csharp/language-reference/operators/conditional-operator
          path = O &gt;= 0 ? path.CullFirstElement() : path.CullElement();
        }
        // Adding the data to the variable tree using the modified path.
        tree.AddRange(inputTree.Branches[i], path);
      }
    }
    // Output result.
    Dr = tree;</item>
