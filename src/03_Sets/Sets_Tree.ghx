                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component redirects a stream into specific output.
    // This component acts like a switch, the input is allowed to pass through the component into a specific output.

    // NOTE
    // Since we are working with dynamic outputs in this component,
    // the output parameter is set in the AfterRunScript()function. Hence, before every execution
    // of the RunScript() function, we manually clear out all the data in the output.
    // We identify the internal variable using underscore.

    // If the input G, input index 0, is empty, so no values have been passed to input.
    // We throw a warning message.
    if(this.Component.Params.Input[0].VolatileData.IsEmpty)
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, "Input parameter S failed to collect data");
    // If the input G, input index 0, has more than one value.
    // We set the flagGateInputs true, and throw an error.
    if(this.Component.Params.Input[1].VolatileData.DataCount &gt; 1)
    {
      _flagGateInputs = true;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Gate component can only operate on a single Gate Index value.");
    }
      // If the input G value is less than zero.
      // We set the flag false, and throw an error.
    else if(G &lt; 0)
    {
      _flag = false;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Gate index too low.");
    }
      // If the input G value is bigger than the number of input.
      // We reduce the count by one because the ouputs start with index zero.
      // We set the flag false, and throw an error.
    else if(G &gt; this.Component.Params.Input.Count - 1)
    {
      _flag = false;
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Gate index too high.");
    }
      // If we passed all the check we set the flag true and the gateValue equal to the G input.
    else
    {
      _flag = true;
      _gateValue = G;
    }</item>
