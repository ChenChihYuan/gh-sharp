                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component matches the path structure to another.

    // Glabal variables.
    // We collect the two inputs structure.
    // We instantiate a DataTree.
    // We collect the length or dimension of the input tree.
    GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[0].VolatileData;
    GH_Structure&lt;IGH_Goo&gt; treeGuide = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[1].VolatileData;
    DataTree&lt;IGH_Goo&gt; tree = new DataTree&lt;IGH_Goo&gt;();
    int treeCapacity = inputTree.PathCount;

    // We check if the two trees have the same path dimensions,
    // if it isn't we throw an error and we exit the process.
    if(inputTree.PathCount != treeGuide.PathCount)
    {
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Input trees must have equal number of paths");
      return;
    }
    else
    {
      // If the paths match, we add to our variable tree the branch from the input tree and the path from the treeGuide.
      for (int i = 0; i &lt; treeCapacity; i++)
        tree.AddRange((IEnumerable&lt;IGH_Goo&gt;) inputTree.Branches[i], treeGuide.get_Path(i));
    }
    // Output result.
    Tr = tree;</item>
