              <items count="2">
                <item name="GUID" type_name="gh_guid" type_code="9">a9a8ebd2-fff5-4c44-a8f5-739736d129ba</item>
                <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
              </items>
              <chunks count="1">
                <chunk name="Container">
                  <items count="10">
                    <item name="CustomUsing" type_name="gh_bool" type_code="1">true</item>
                    <item name="Description" type_name="gh_string" type_code="10">A C#.NET scriptable component</item>
                    <item name="EditorPosition" type_name="gh_drawing_point" type_code="30">
                      <X>61</X>
                      <Y>98</Y>
                    </item>
                    <item name="IconDisplay" type_name="gh_int32" type_code="3">1</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">e6ddeaa4-291d-4214-a7f3-4affc69efec6</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">GH# Trim</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">false</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component reduces the complexity of the tree,
    // merging the last index of the path and moving to the first stepping the number of times is defined by the value D.

    // Global parameter
    DataTree&lt;object&gt; trimedTree = new DataTree&lt;object&gt;();

    // First step we are checking that we are not taking more than one value for the input D.
    if(this.Component.Params.Input[1].VolatileDataCount &gt; 1)
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, "This component only accepts a snigle number for Depth");

      // Second step is to check that input D is not less than zero.
    else if(D &lt; 0)
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, "Depth has to be a positive integer");

    else
    {
      int counter = 0;
      // We loop through the branches.
      for (int i = 0; i &lt; T.BranchCount; i++)
      {
        // Collecting the path and data for each branch.
        GH_Path path = T.Path(i);
        List&lt;object&gt; branch = T.Branch(i);
        // If the path length is smaller than the input increment the counter of one;
        if(path.Length &lt;= D)
          ++counter;
        else
        {
          // GH_Path class has a method to remove the last index of the path.
          // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_Data_GH_Path_CullElement.htm
          // So we call the method inside a loop for the number of times is defined by the input D.
          for (int j = 0; j &lt; D; j++)
          {
            path = path.CullElement();
          }
          // We add the values to the DataTree using the 'trimmed' path.
          trimedTree.AddRange(branch, path);
        }
      }
      // If the path length is smaller than input D trigger the counter.
      // If the counter is equal or bigger than one, we throw a warning message.
      if(counter &gt;= 1)
      {
        string warningMessage = string.Format("The collapse depth is too large for {0} branches, they were omitted", counter);
        this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, warningMessage);
      }
    }
    // Output result.
    Tr = trimedTree;</item>
