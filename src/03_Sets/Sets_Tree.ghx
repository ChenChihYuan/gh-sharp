                    <item name="EditorPosition" type_name="gh_drawing_point" type_code="30">
                      <X>5</X>
                      <Y>107</Y>
                    </item>
                    <item name="IconDisplay" type_name="gh_int32" type_code="3">1</item>
                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">571ebdcf-7d32-4fb4-8f00-ac0022dbf5b9</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">GH# Clean</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">false</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component removes all the null and invalid values from the tree structure.

    // The first step is to check if the bool inputs are single values and not lists.
    // To check this, we take the information directly from the input parameters of the C# component.
    // Retrieving the VolatileDataCount we can have the count of values passed from each input.
    for (int i = 0; i &lt;= 2; i++)
    {
      IGH_Param param = this.Component.Params.Input[i];
      if (param.VolatileDataCount &gt; 1)
        this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, string.Format("This component only accepts a single value for [{0}].", (string) param.NickName));
    }

    // If input N and X are true we enter in the if statement.
    if(N || X)
    {
      // Each branch in a DataTree is a list, and we have to declare the type of the list.
      // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_DataTree_1_Branch_1.htm
      // In our case the type of the list is an object because we want to keep the type as generic as possible starting from the DataTree.
      foreach (List&lt;object&gt; branch in T.Branches)
      {
        // If the branch is not null we enter in the statement.
        if(branch != null)
        {
          // Here a little trick to remove elements from a list without having problems with the order,
          // starting from the end of the list and moving backward.
          for (int j = branch.Count - 1; j &gt;= 0; j += -1)
          {
            // We set a boolean value that we will use to trigger the action of removing the elements we want.
            bool flag = false;
            // This is another little trick for taking advantage of the IsValid property provided by the IGH_Goo
            // that is the interface for all the data inside Grasshopper.
            // https://developer.rhino3d.com/api/grasshopper/html/T_Grasshopper_Kernel_Types_IGH_Goo.htm
            // To obtain an IGH_Goo Grasshopper has a dedicated class to convert data into specific Goo, GH_Convert.
            // https://developer.rhino3d.com/api/grasshopper/html/T_Grasshopper_Kernel_GH_Convert.htm
            IGH_Goo tempValue = GH_Convert.ToGoo(branch[j]);
            // If the value is null and the input N is true we want to remove the null values, so we set the flag = true.
            // We use the property isValid to check the validity of the value
            // and if the input X is true, we set the flag = true.
            // Little note, because IsValid retrieves a true value we have to negate a false result to have true when the value is not valid.
            if(tempValue == null)
              flag = N;
            else if(X)
              flag = !tempValue.IsValid;
            // At the end if the flag is true, we remove the value at the index j.
            if(flag)
              branch.RemoveAt(j);
          }
        }
      }
    }
    // If E is true, so we want to remove empty branches.
    if(E)
    {
      // We use the same little trick as before so we start from the end and move backward.
      for (int i = T.Branches.Count - 1; i &gt;= 0; i += -1)
      {
        // If the count of a branch (list of values) is less or equal to zero, meaning the list is empty.
        // Using RemovePath we remove the entire branch, from the DataTree.
        if(T.Branch(i).Count &lt;= 0)
          T.RemovePath(i);
      }
    }

    // Output result.
    Tr = T;</item>
