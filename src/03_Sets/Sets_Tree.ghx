                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  // Method used to modify the input names.
  public void VariableParameterMaintenance()
  {
    // We loop through the inputs.
    int num = this.Component.Params.Input.Count - 1;
    for (int index = 0; index &lt;= num; ++index)
    {
      // We set the NickName and Name of the inputs.
      // NickName is the 'name' visible on the component.
      // Name is the 'name' in the description when you put the mouse on an input or output.
      this.Component.Params.Input[index].Name = string.Format("Branch {0}", index);
      this.Component.Params.Input[index].NickName = string.Format("D{0}", index + 1);
      // We set the input access, this is the same action we do when we right click
      // on a input or output and we select an item a list or a tree, in this case we select a tree.
      this.Component.Params.Input[index].Access = GH_ParamAccess.tree;
    }
  }
  // Inside the CallBack we call the method we defined to change the input names, and we expire the component solution so
  // in the next new solution this component can be solved again.
  // It is necessary to define the CallBack method with a GH_Document input otherwise the ScheduleSolution won't work.
  public void CallBack(GH_Document gh)
  {
    VariableParameterMaintenance();
    this.Component.Params.OnParametersChanged();
    this.Component.ExpireSolution(false);
  }

  // Private method to convert a GH_Structure in DataTree.
  private DataTree&lt;IGH_Goo&gt; StructureToDataTree(GH_Structure&lt;IGH_Goo&gt; structure)
  {
    // We instantiate a tree.
    DataTree&lt;IGH_Goo&gt; tree = new DataTree&lt;IGH_Goo&gt;();
    // We loop through each branch and copied in the DataTree.
    for(int i = 0;i &lt; structure.Branches.Count;++i)
    {
      tree.AddRange((List&lt;IGH_Goo&gt;) structure.get_Branch(i), structure.get_Path(i));
    }
    // We return the tree.
    return tree;
  }</item>
