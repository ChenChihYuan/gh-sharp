                    <item name="InstanceGuid" type_name="gh_guid" type_code="9">14da4790-9771-4e56-9b5f-85d96cd6d616</item>
                    <item name="Name" type_name="gh_string" type_code="10">C# Script</item>
                    <item name="NickName" type_name="gh_string" type_code="10">GH# Split</item>
                    <item name="OutParameter" type_name="gh_bool" type_code="1">false</item>
                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">    // Component written by @sonomirco

    // The component splits a data tree into two parts using path masks.
    // You can have more information on path masks using the link below.
    // https://discourse.mcneel.com/t/c-component-with-split-tree-input/44674
    // And here a very brief description of how the split tree works.
    // https://discourse.mcneel.com/t/c-component-with-split-tree-input/44674/3?u=bianchini

    // Glabal variables.
    // We collect the data directly from the inputs so we can make better checks on the data e structure.
    GH_Structure&lt;IGH_Goo&gt; inputTree = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[0].VolatileData;
    GH_Structure&lt;IGH_Goo&gt; inputMask = (GH_Structure&lt;IGH_Goo&gt;) this.Component.Params.Input[1].VolatileData;
    // We initialize two data trees of type IGH_Goo, so we can handle all the null and empty data.
    DataTree&lt;IGH_Goo&gt; positiveTree = new DataTree&lt;IGH_Goo&gt;();
    DataTree&lt;IGH_Goo&gt; negativeTree = new DataTree&lt;IGH_Goo&gt;();
    // We initialize a list of GH_TreeRules, which is the core of the split tree.
    // https://developer.rhino3d.com/api/grasshopper/html/T_Grasshopper_Kernel_Data_GH_TreeRules.htm
    List&lt;GH_TreeRules&gt; treeRules = new List&lt;GH_TreeRules&gt;();
    // And we initialize two boolean values that we will use to filter some behaviors.
    bool pathFlag = false;
    bool addFlag = false;

    // We throw a warning message if we don't have any data passed to the input M
    if(inputMask.IsEmpty)
    {
      this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Warning, "Input parameter M failed to collect data");
      return;
    }
    else
    {
      // We initialize two strings.
      // message, will collect the error message from the method FromString.
      // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_Data_GH_TreeRules_FromString.htm
      // ruleString, will collect the conversation from a IGH_Goo to a string.
      // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_GH_Convert_ToString.htm
      string message = null;
      string ruleString = string.Empty;
      // We loop through the list of masks.
      foreach (IGH_Goo rule in inputMask.Branches[0])
      {
        // We convert the IGH_Goo into a string using the converter.
        GH_Convert.ToString(rule, out ruleString, GH_Conversion.Primary);
        // We create a tree rule from a string.
        GH_TreeRules treeRule = GH_TreeRules.FromString(ruleString, ref message);
        // If we catch a message we throw it as an error.
        if(message != null)
          this.Component.AddRuntimeMessage(GH_RuntimeMessageLevel.Error, message);
        // If the rule is valid we store it in treeRules list.
        if(treeRule != null)
        {
          treeRules.Add(treeRule);
          // If the rule is valid and is index rule, we flag the first boolean.
          // https://developer.rhino3d.com/api/grasshopper/html/P_Grasshopper_Kernel_Data_GH_TreeRules_HasItemRule.htm
          if(treeRule.HasItemRule)
            pathFlag = true;
        }
        // If the list of rules is empty, we output the input tree and terminate the process.
        if(treeRules.Count &lt;= 0)
        {
          // We expire the solution of the output P, to avoid null data as result.
          this.Component.Params.Output[0].ExpireSolution(true);
          N = D;
          return;
        }
      }
      // We loop through the tree dimension, number of branches.
      for (int i = 0; i &lt; inputTree.Branches.Count; i++)
      {
        // We collect the data of the branch by index.
        List&lt;IGH_Goo&gt; tempBranch = inputTree.Branches[i];
        // We collect the path of the branch by index.
        GH_Path path = inputTree.get_Path(i);
        // If this flag is true we jump in this process.
        if(pathFlag)
        {
          // We initialize two lists, and we start to loop every branch.
          List&lt;IGH_Goo&gt; tempConteiner1 = new List&lt;IGH_Goo&gt;();
          List&lt;IGH_Goo&gt; tempConteiner2 = new List&lt;IGH_Goo&gt;();
          for (int j = 0; j &lt; tempBranch.Count; j++)
          {
            // We reset the value to false every interaction.
            addFlag = false;
            // We loop through the tree rules we created.
            foreach (GH_TreeRules rule in treeRules)
            {
              // We check if the rule is appliable, if it is true, we flag our second boolean value and we break the loop.
              // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_Data_GH_TreeRules_Apply_1.htm
              if (rule.Apply(path, j))
              {
                addFlag = true;
                break;
              }
            }
            // If our second flag is true, we collect the data in the first container and we add a null value in the second.
            // If it is false is the opposite.
            if(addFlag)
            {
              tempConteiner1.Add(tempBranch[j]);
              tempConteiner2.Add((IGH_Goo) null);
            }
            else
            {
              tempConteiner1.Add((IGH_Goo) null);
              tempConteiner2.Add(tempBranch[j]);
            }
          }
          // We collect the list in the data tree.
          positiveTree.AddRange(tempConteiner1, path);
          negativeTree.AddRange(tempConteiner2, path);
        }
          // If we don't have rules using index rule. We step in this condition.
        else
        {
          // We reset the value to false every interaction.
          addFlag = false;
          // We loop through the tree rules we created.
          foreach (GH_TreeRules rule in treeRules)
          {
            // We check if the rule is appliable, if it is true, we flag our second boolean value and we break the loop.
            // https://developer.rhino3d.com/api/grasshopper/html/M_Grasshopper_Kernel_Data_GH_TreeRules_Apply.htm
            if (rule.Apply(path))
            {
              addFlag = true;
              break;
            }
          }
          // If our second flag is true, we collect the data in the positive tree.
          // If it is false, we collect the data in the negative tree.
          if(addFlag)
            positiveTree.AddRange(inputTree.Branches[i], path);
          else
            negativeTree.AddRange(inputTree.Branches[i], path);
        }
      }
    }
    // Output results.
    P = positiveTree;
    N = negativeTree;</item>
